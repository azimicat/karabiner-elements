{
    "title": "Emoji-key (non-apple keyboard [not fn])",
    "rules": [
        {
            "description": "Press right_alt alone toggles emoji input",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "f6",
                        "modifiers": {
                            "optional": "any"
                        }
                    },
                    "to": [
                        {
                            "key_code": "right_alt"
                        }
                    ],
                    "to_if_alone": [
                        {
                            "key_code": "fn"
                        }
                    ]
                }
            ]
        }
    ]
}
